  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "spatie/phpunit-watcher": "~1.24"
  },
  "autoload": {
    "psr-4": {
      "Lacus\\Validators\\Cpf\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lacus\\Validators\\Cpf\\Tests\\": "tests/"
    }
  }
