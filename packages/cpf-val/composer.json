{
  "name": "lacus/cpf-val",
  "version": "0.0.1",
  "type": "library",
  "description": "Utility function to validate CPF (Brazilian personal ID)",
  "license": "MIT",
  "authors": [
    {
      "name": "Julio L. Muller",
      "email": "juliolmuller@outlook.com",
      "homepage": "https://juliolmuller.github.io"
    }
  ],
  "keywords": [
    "cpf",
    "format",
    "formatting",
    "formatar",
    "mascara",
    "mask",
    "pt-br",
    "br"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/LacusSolutions/br-utils-php.git",
    "directory": "packages/cpf-val/"
  },
  "support": {
    "issues": "https://github.com/LacusSolutions/br-utils-php/issues",
    "source": "https://github.com/LacusSolutions/br-utils-php"
  },
  "homepage": "https://cpf-utils.vercel.app/",
  "scripts": {
    "test": "phpunit",
    "test:watch": "phpunit-watcher watch",
    "test-coverage": "phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=8.1",
    "lacus/cpf-gen": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "spatie/phpunit-watcher": "~1.24"
  },
  "autoload": {
    "psr-4": {
      "Lacus\\CpfVal\\": "src/"
    },
    "files": [
      "src/cpf-val.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Lacus\\CpfVal\\Tests\\": "tests/"
    }
  }
}
