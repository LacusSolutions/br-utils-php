{
  "name": "lacus/br-utils",
  "type": "library",
  "description": "Utility resources to deal with Brazilian-related data.",
  "license": "MIT",
  "authors": [
    {
      "name": "Julio L. Muller",
      "email": "juliolmuller@outlook.com",
      "homepage": "https://juliolmuller.github.io"
    }
  ],
  "keywords": [
    "cpf",
    "cnpj",
    "cep",
    "rg",
    "state",
    "city",
    "validar",
    "validador",
    "validate",
    "validation",
    "formatar",
    "format",
    "gerar",
    "gerador",
    "generate",
    "generator",
    "pt-br",
    "br"
  ],
  "support": {
    "issues": "https://github.com/LacusSolutions/br-utils-php/issues",
    "source": "https://github.com/LacusSolutions/br-utils-php"
  },
  "homepage": "https://br-utils.vercel.app/",
  "scripts": {
    "test": "phpunit",
    "test:watch": "phpunit-watcher watch",
    "test-coverage": "phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=8.1",
    "lacus/cnpj-fmt": "^1.0",
    "lacus/cnpj-gen": "^1.0",
    "lacus/cnpj-utils": "^1.1",
    "lacus/cnpj-val": "^1.0",
    "lacus/cpf-fmt": "^1.0",
    "lacus/cpf-gen": "^1.0",
    "lacus/cpf-utils": "^1.1",
    "lacus/cpf-val": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "spatie/phpunit-watcher": "~1.24"
  },
  "autoload": {
    "psr-4": {
      "Lacus\\BrUtils\\": "src/"
    },
    "files": [
      "src/Cnpj/cnpj_utils.php",
      "src/Cpf/cpf_utils.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Lacus\\BrUtils\\Tests\\": "tests/",
      "Lacus\\CnpjUtils\\Tests\\": "vendor/lacus/cnpj-utils/tests/",
      "Lacus\\CpfUtils\\Tests\\": "vendor/lacus/cpf-utils/tests/",
      "Lacus\\CnpjFmt\\Tests\\": "vendor/lacus/cnpj-fmt/tests/",
      "Lacus\\CnpjGen\\Tests\\": "vendor/lacus/cnpj-gen/tests/",
      "Lacus\\CnpjVal\\Tests\\": "vendor/lacus/cnpj-val/tests/",
      "Lacus\\CpfFmt\\Tests\\": "vendor/lacus/cpf-fmt/tests/",
      "Lacus\\CpfGen\\Tests\\": "vendor/lacus/cpf-gen/tests/",
      "Lacus\\CpfVal\\Tests\\": "vendor/lacus/cpf-val/tests/"
    }
  }
}
