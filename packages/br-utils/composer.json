{
  "name": "lacus/br-utils",
  "type": "library",
  "description": "Utility resources to deal with Brazilian-related data.",
  "license": "MIT",
  "authors": [
    {
      "name": "Julio L. Muller",
      "email": "juliolmuller@outlook.com",
      "homepage": "https://juliolmuller.github.io"
    }
  ],
  "keywords": [
    "cpf",
    "cnpj",
    "cep",
    "rg",
    "state",
    "city",
    "validar",
    "validador",
    "validate",
    "validation",
    "formatar",
    "format",
    "gerar",
    "gerador",
    "generate",
    "generator",
    "pt-br",
    "br"
  ],
  "support": {
    "issues": "https://github.com/LacusSolutions/br-utils-php/issues",
    "source": "https://github.com/LacusSolutions/br-utils-php"
  },
  "homepage": "https://cpf-utils.vercel.app/",
  "scripts": {
    "test": "phpunit",
    "test:watch": "phpunit-watcher watch",
    "test-coverage": "phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "spatie/phpunit-watcher": "~1.24"
  },
  "autoload": {
    "psr-4": {
      "Lacus\\BrUtils\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lacus\\BrUtils\\Tests\\": "tests/"
    }
  }
}
